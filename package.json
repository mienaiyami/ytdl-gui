{
  "name": "ytdl-gui",
  "productName": "ytdl-gui",
  "version": "0.0.3",
  "description": "Add to download youtube audio or video",
  "main": ".vite/build/main.js",
  "scripts": {
    "preci": "rm -rf ./node_modules",
    "ci": "yarn install --immutable --immutable-cache --check-cache",
    "dev": "npm run start",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "make": "electron-forge make --arch=ia32,x64",
    "make:exe": "electron-forge make --targets=@electron-forge/maker-squirrel --arch=ia32",
    "make:zip": "electron-forge make --targets=@electron-forge/maker-zip --arch=ia32",
    "make:deb": "electron-forge make --targets=@electron-forge/maker-deb",
    "release": "node release.js"
  },
  "keywords": [],
  "author": {
    "name": "mienaiyami",
    "email": "mienaiyami0@gmail.com",
    "url": "https://github.com/mienaiyami/ytdl-gui"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/plugin-vite": "^6.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.15",
    "electron": "25.3.2",
    "eslint": "^8.46.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.28",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
